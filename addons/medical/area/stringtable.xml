			<Key ID="STR_AXE_Medical_Area_Progress_LightOff">
				<Original>Remove Lights...</Original>
				<German>Beleuchtung abbauen...</German>
			</Key>
			<Key ID="STR_AXE_Medical_Area_Progress_LightOn">
