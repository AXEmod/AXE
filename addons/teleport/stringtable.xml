		<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
		<Container name="Teleport_Module">
			<!-- =============================================================================== -->
			<Key ID="STR_AXE_Teleport_Module_displayName">
				<Original>Teleport Network</Original>
				<German>Teleport Netzwerk</German>
			</Key>
			<Key ID="STR_AXE_Teleport_Module_Description">
				<Original>Creates a network of synchronized teleport flags.</Original>
				<German>Erstellt ein Netzwerk aus synchronisierten Teleport Flaggen.</German>
			</Key>
			<!-- =============================================================================== -->
			<Key ID="STR_AXE_Teleport_Module_Property_Enabled_Title">
				<Original>Enabled</Original>
				<German>Aktiviert</German>
			</Key>
			<Key ID="STR_AXE_Teleport_Module_Property_Enabled_Tip">
				<Original>Enables/disables the entire teleport network.</Original>
				<German>Aktiviert/Deaktiviert das gesamte Teleport-Netzwerk.</German>
			</Key>
			<Key ID="STR_AXE_Teleport_Module_Property_Enabled_0">
				<Original>false</Original>
				<German>false</German>
			</Key>
			<Key ID="STR_AXE_Teleport_Module_Property_Enabled_1">
				<Original>true</Original>
				<German>true</German>
			</Key>
			<!-- =============================================================================== -->
		</Container>
